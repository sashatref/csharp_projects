#include "buildNumber.h"


1 VERSIONINFO
FILEVERSION MAJOR_VERSION,MINOR_VERSION,MAINTENANCE,BUILD
FILEFLAGSMASK 0x17L
FILEOS 0x4L
FILETYPE 0x1L
FILESUBTYPE 0x0L
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "040904b0"
		BEGIN
			VALUE "FileDescription", "My application description"
			VALUE "LegalCopyright", "My coorporation"
			VALUE "OriginalFilename", "myApp.exe"
			VALUE "ProductName", "myApp"
			VALUE "ProductVersion", PRODUCT_VER
		END
	END
	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x419, 1252
	END
END